<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="windows" stylesheets="@../Dashboard.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Project.CreateFileController">
   <children>
      <Label layoutX="151.0" layoutY="84.0" styleClass="login-label" stylesheets="@../Dashboard.css" text="Upload a new file">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <TextField fx:id="filePathTextField" layoutX="68.0" layoutY="212.0" prefHeight="22.0" prefWidth="384.0" styleClass="login-textfield" stylesheets="@../Dashboard.css" />
      <Button fx:id="browseButton" layoutX="463.0" layoutY="210.0" mnemonicParsing="false" onMouseClicked="#openFileChooser" prefHeight="38.0" prefWidth="89.0" styleClass="login-button" stylesheets="@../Dashboard.css" text="Browse..." />
      <Button layoutX="413.0" layoutY="313.0" mnemonicParsing="false" onMouseClicked="#createFileClicked" onMouseEntered="#checkIfEmptyTextField" prefHeight="25.0" prefWidth="119.0" styleClass="login-button" stylesheets="@../Dashboard.css" text="Upload file" />
      <Label fx:id="errorLabel" layoutX="68.0" layoutY="317.0" text="You can't upload a file with no name!" textFill="#9a0e0e" visible="false">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
