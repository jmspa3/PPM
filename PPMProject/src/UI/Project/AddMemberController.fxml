<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="windows" stylesheets="@../Dashboard.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Project.AddMemberController">
    <children>
        <Label layoutX="174.0" layoutY="93.0" styleClass="login-label" stylesheets="@../Dashboard.css" text="Add a new member">
            <font>
                <Font size="32.0" />
            </font>
        </Label>
        <TextField fx:id="usernameTextField" layoutX="89.0" layoutY="200.0" prefHeight="33.0" prefWidth="423.0" promptText="Username..." styleClass="login-textfield" stylesheets="@../Dashboard.css">
